{
    "name": "express mangoDB",
    "version": "1.0.0",
    "description": "A webpack-express app",
    "scripts": {
        "start": "cross-env NODE_ENV=dev concurrent --kill-others \"npm run start-dev\" \"npm run start-dev-api\"",
        "start-dev": "webpack-dev-server --inline --progress --config config/webpack.dev.js",
        "start-dev-api": "supervisor -w node --harmony cluster",
